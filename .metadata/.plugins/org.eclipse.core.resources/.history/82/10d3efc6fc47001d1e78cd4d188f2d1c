package homework1;

public class Customer {

    public Customer() {
		
	}
    
    
	
	private int id;
	private String firstName;
	private String lastName;
	private String nationalIdentıty;
	
	public Customer(int id,String firstName,String lastName,String nationalIdentıty){
		this.id=id;
		this.firstName=firstName;
		this.lastName=lastName;
		this.nationalIdentıty=nationalIdentıty;
		
		
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getFirstName() {
		return firstName;
	}
	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}
	public String getLastName() {
		return lastName;
	}
	public void setLastName(String lastName) {
		this.lastName = lastName;
	}
	public String getNationalIdentıty() {
		return nationalIdentıty;
	}
	public void setNationalIdentıty(String nationalIdentıty) {
		this.nationalIdentıty = nationalIdentıty;
	}
	
	
}
